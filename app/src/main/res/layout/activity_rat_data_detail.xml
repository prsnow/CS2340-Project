<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".controller.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/button_close"
        android:layout_width="236dp"
        android:layout_height="48dp"
        android:layout_marginBottom="65dp"
        android:onClick="onClosePressed"
        android:text="Close"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:text="Rat Sighting"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="31dp" />

    <TextView
        android:id="@+id/keyLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:layout_marginBottom="54dp"
        android:layout_marginEnd="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="57dp"
        android:text="Key: N/A"
        android:textAlignment="viewStart"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/locationLabel"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/dateLabel"
        android:layout_width="343dp"
        android:layout_height="20dp"
        android:text="Date: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="112dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"
        app:layout_constraintHorizontal_bias="0.333" />

    <TextView
        android:id="@+id/locationLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Location: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/dateLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/dateLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/dateLabel" />

    <TextView
        android:id="@+id/zipLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Zip: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/locationLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/locationLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/locationLabel" />

    <TextView
        android:id="@+id/addressLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Address: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/zipLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/zipLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/zipLabel" />

    <TextView
        android:id="@+id/cityLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="City: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/addressLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/addressLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/addressLabel" />

    <TextView
        android:id="@+id/boroughLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Borough: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/cityLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/cityLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/cityLabel" />

    <TextView
        android:id="@+id/latitudeLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Latitude: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/boroughLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/boroughLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/boroughLabel" />

    <TextView
        android:id="@+id/longitudeLabel"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Longitude: N/A"
        android:textAlignment="viewStart"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/latitudeLabel"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/latitudeLabel"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/latitudeLabel" />

</android.support.constraint.ConstraintLayout>
